<?xml version="1.0" encoding="utf-8" ?>
<odoo>

     <!-- Set Up Responsive Logo Show -->
    <template id="THEME_NAME_logo_show" name="Logo Show THEME_NAME Template" inherit_id="website_multi_theme.auto_view_THEME_NAME_website_layout_logo_show">
        <xpath expr="//a[hasclass('navbar-brand')]/span" position="replace">
            <img class="img img-responsive" src="/theme_canilike/static/img/canilike/img-logo.png" alt="website.name"/>
        </xpath>
    </template>

</odoo>